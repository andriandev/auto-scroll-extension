{
  "manifest_version": 3,
  "name": "Auto Scroll Extension",
  "version": "1.0",
  "description": "Scroll web page automatically",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  }
}
